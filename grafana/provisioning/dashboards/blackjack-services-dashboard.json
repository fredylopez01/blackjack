    {
      "type": "gauge",
      "title": "API Login",
      "id": 1,
      "datasource": null,
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 1},
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {"mode": "continuous-RdYlGn"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 0.5},
              {"color": "green", "value": 0.99}
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(up{job=\"api-login\"}[5m])",
          "legendFormat": "API Login"
